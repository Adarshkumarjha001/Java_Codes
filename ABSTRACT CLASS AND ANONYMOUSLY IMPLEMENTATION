


abstract class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    
}
class B extends A{
    public void show(){
        System.out.println("Show is in B new  class");
    }
}

public class Main
{
	public static void main(String[] args) {
		A obj=new B();
	
		obj.show();
	}
}


abstract class A{
    
    abstract public void show();
    abstract public void fun();
       
    
}

public class Main
{
	public static void main(String[] args) {
		A obj=new A(){
		     public void show(){
        System.out.println("Show is in B new  class");
    }
    public void fun(){
        System.out.println("fun is in B new  class");
    }
		};
	
		obj.show();
		obj.fun();
	}
}





abstract class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    
}

public class Main
{
	public static void main(String[] args) {
		A obj=new A(){
		     public void show(){
        System.out.println("Show is in B new  class");
    }
		};
	
		obj.show();
	}
}

