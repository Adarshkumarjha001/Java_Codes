


abstract class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    
}
class B extends A{
    public void show(){
        System.out.println("Show is in B new  class");
    }
}

public class Main
{
	public static void main(String[] args) {
		A obj=new B();
	
		obj.show();
	}
}






