
OVERIDING METHOD

class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    
}
class B extends A{
    public void show(){
        System.out.println("Show is in B  class me hu");
    }
}
public class Main
{
	public static void main(String[] args) {
		A obj=new B();
	
		obj.show();
	}
}




ANONYMOUS CLASS


class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    
}

public class Main
{
	public static void main(String[] args) {
		A obj=new A(){
		    public void show(){
        System.out.println("Show is in b NEW  class");
    }
    
		};
	
		obj.show();
	}
}



