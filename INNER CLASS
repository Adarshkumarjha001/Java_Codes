



class A{
    int a=6;
    public void show(){
        System.out.println("Show is in A class");
    }
    class B{
        public void fun(){
            System.out.println("Fun in class B");
        }
    }
}
public class Main
{
	public static void main(String[] args) {
		A obj=new A();
		A.B obj1=obj.new B();
		obj1.fun();
		obj.show();
	}
}
